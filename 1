--antiafk

local player = game.Players.LocalPlayer
local lastMove = tick()

-- Function that checks if the player has moved recently
local function checkAFK()
    if tick() - lastMove > 600 then -- 10 minutes
        player.Character:BreakJoints()
        print("Player has been kicked for being AFK.")
    end
end

-- Connect the function to the player's character added event
player.CharacterAdded:Connect(function()
    -- When the player's character is added, reset the lastMove timer
    lastMove = tick()
    -- Connect the checkAFK function to the player's character's humanoid's running event
    player.Character.Humanoid.Running:Connect(function()
        lastMove = tick()
    end)
end)

-- Repeatedly call the checkAFK function every 60 seconds
while true do
    checkAFK()
    wait(60)
end
